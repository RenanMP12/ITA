   "source": [
    "from sympy import *\n",
    "\n",
    "L, E, I, q, v2, theta2, theta3 = symbols('L E I q v2 theta2 theta3')\n",
    "\n",
    "kg = E*I/L**3*Matrix([[36, -6*L, -12],[-6*L, 12*L**2, -6*L],[6*L, 2*L**2, 4*L**2]])\n",
    "fg = Matrix([[q*L],[q*L**2/6],[0]])\n",
    "\n",
    "sol = linsolve((kg, fg), [v2, theta2, theta3])"
   ]
  },
